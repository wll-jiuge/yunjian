<?xml version="1.0" encoding="utf-8"?>
<!--日志列表显示-->
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:background="@drawable/addxunjian_bgshape"
    android:layout_height="wrap_content">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <ImageView
            android:layout_margin="@dimen/dimen_15dp"
            android:id="@+id/imageView10"
            android:layout_width="@dimen/dimen_15dp"
            android:layout_height="@dimen/dimen_15dp"
            app:srcCompat="@mipmap/ic_loginfomation_log_selected" />

        <TextView
            android:id="@+id/loginfo_list_min_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dimen_15dp"
            android:text="test" />

        <TextView
            android:id="@+id/loginfo_list_min_situation"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@color/loginfo_list_min_situation"
            android:textColor="@color/color_white"
            android:layout_marginTop="@dimen/dimen_15dp"
            android:layout_marginLeft="@dimen/dimen_300dp"
            android:text="良好" />
    </LinearLayout>

    <TextView
        android:id="@+id/loginfo_list_progress"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dimen_15dp"
        android:layout_marginRight="@dimen/dimen_15dp"
        android:text="TextView" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_vertical"
        android:layout_marginLeft="@dimen/dimen_15dp"
        android:layout_marginRight="@dimen/dimen_15dp"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/imageView11"
            android:layout_width="@dimen/dimen_15dp"
            android:layout_height="@dimen/dimen_15dp"
            app:srcCompat="@drawable/icon_clock" />

        <TextView
            android:id="@+id/loginfo_list_min_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="TextView" />

        <TextView
            android:id="@+id/loginfo_list_min_weather"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="TextView" />
    </LinearLayout>
</LinearLayout>